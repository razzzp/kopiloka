
<h1>Edit <%= cafe.name %>
</h1>
<form action="/cafes/<%= cafe._id %>?_method=PUT" method="POST" class="needs-validation" novalidate>
  <%- include('form') %>
  <div class="row g-2 justify-content-start">
    <div class="col-2">
      <button type="submit" class="btn btn-primary mt-3 w-100">Save</button>
    </div>
    <div class="col-2">
      <a href="/cafes/<%= cafe._id %>" class="btn btn-primary mt-3 w-100">Cancel</a>
    </div>
  </div>
</form>
